{
  "name": "st_backend",
  "version": "1.0.0",
  "description": "express server",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=prod node dist/app.js dotenv -e .env.prod",
    "start:dev": "NODE_ENV=local npm run build:live",
    "build": "rimraf ./dist && tsc -p .",
    "build:live": "nodemon --watch 'src/**/*.ts' --exec \"ts-node\" src/app.ts",
    "typeorm": "node ./node_modules/typeorm/cli.js",
    "typeorm:dev": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gong-yeongbin/st_backend.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/gong-yeongbin/st_backend/issues"
  },
  "homepage": "https://github.com/gong-yeongbin/st_backend#readme",
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.15",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^18.11.18",
    "@types/node-schedule": "^2.1.0",
    "@types/node-telegram-bot-api": "^0.57.6",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.2",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "moment-timezone": "^0.5.40",
    "mongoose": "^6.8.3",
    "mysql": "^2.18.1",
    "node-schedule": "^2.1.1",
    "node-telegram-bot-api": "^0.61.0",
    "redis": "^4.6.5",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.12"
  }
}
